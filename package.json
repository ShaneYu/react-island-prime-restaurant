{
  "name": "react-island-prime-restaurant",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/ShaneYu/react-island-prime-restaurant",
  "bugs": {
    "url": "https://github.com/ShaneYu/react-island-prime-restaurant/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaneYu/react-island-prime-restaurant.git"
  },
  "license": "MIT",
  "author": {
    "name": "Shane Yu",
    "url": "https://github.com/ShaneYu"
  },
  "scripts": {
    "build": "react-scripts build",
    "display-browser-support": "browserslist --stats && echo '' && browserslist --coverage && echo '' && echo 'See more at https://browserl.ist/'",
    "format": "run-s format-doctoc format-package-json format-imports format-prettier format-eslint",
    "format-doctoc": "doctoc --maxlevel 3 --notitle ./README.md ./CONTRIBUTING.md",
    "format-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --fix ./",
    "format-imports": "import-sort --write **/*.{js,jsx,ts,tsx}",
    "format-package-json": "sort-package-json",
    "format-prettier": "prettier --write **/*.{css,html,js,jsx,json,less,md,mdx,scss,ts,tsx,yaml,yml} .editorconfig LICENSE",
    "lint": "run-p lint-eslint lint-markdown",
    "lint--watch": "onchange src/**/* --initial --kill --delay 1000  -- npm run lint",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint-markdown": "markdownlint --ignore coverage --ignore build --ignore node_modules **/*.md",
    "prepublish": "rimraf build && npm run build",
    "start": "run-p --race start:server start:app",
    "start:app": "cross-env PUBLIC_URL=/ react-scripts start",
    "start:server": "cd ./server && ts-node-dev --project ./tsconfig.json ./server.ts && wait-on tcp:$SERVER_PORT",
    "test": "react-scripts test",
    "validate": "run-p --print-label lint test build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/history": "^4.7.6",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.151",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.35",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.8",
    "@types/react-redux-toastr": "^7.6.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-saga": "^0.10.5",
    "@types/reselect": "^2.2.0",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "axios": "^0.19.2",
    "axios-retry": "^3.1.8",
    "bootstrap": "^4.4.1",
    "connected-react-router": "^6.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "doctoc": "^1.4.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "eslint-formatter-pretty": "^3.0.1",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "husky": "^4.2.5",
    "immer": "^9.0.6",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-config": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^10.2.2",
    "lodash": "^4.17.19",
    "map.prototype.tojson": "^0.1.3",
    "markdownlint-cli": "^0.23.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-loading-skeleton": "^2.0.1",
    "react-parallax": "^3.0.3",
    "react-redux": "^7.2.0",
    "react-redux-toastr": "^7.6.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "sort-package-json": "^1.42.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "typescript-fsa-redux-saga": "^2.0.0"
  }
}
